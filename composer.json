{
    "name": "conia/route",
    "description": "PSR-7/PSR-15 router and dispatcher",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Conia\\Route\\": "src/"
        },
        "files": [
            "src/Utils.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Conia\\Route\\Tests\\": "tests/"
        },
        "files": [
            "tests/Fixtures/globalTestSymbols.php"
        ]
    },
    "authors": [
        {
            "name": "ebene fÃ¼nf GmbH",
            "email": "info@ebenefuenf.de"
        }
    ],
    "require": {
        "php": ">=8.3",
        "conia/wire": "^0.1.0",
        "psr/container": "^2.0",
        "psr/http-message": "^1.1 || ^2.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "conia/development": "^1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "laminas/laminas-diactoros": "^3.3",
        "phpunit/phpunit": "^10.4",
        "vimeo/psalm": "^5.15"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
